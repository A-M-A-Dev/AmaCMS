<% if(currentUser) { %>
    <h3>you're logged in</h3>
    <ul>
        <li><%= currentUser.firstName %></li>
        <li><%= currentUser.lastName %></li>
        <li><%= currentUser.role %></li>
        <li><%= currentUser.email %></li>
    </ul>
<% } else { %>
    <h3>you're not logged in</h3>
<% } %>